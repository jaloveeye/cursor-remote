{
  "name": "cursor-remote-extension",
  "displayName": "Cursor Remote",
  "description": "Remote control extension for Cursor IDE via WebSocket - Code anywhere, anytime with Cursor CLI",
  "version": "0.3.1",
  "publisher": "jaloveeye",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaloveeye/cursor-remote.git"
  },
  "homepage": "https://github.com/jaloveeye/cursor-remote",
  "bugs": {
    "url": "https://github.com/jaloveeye/cursor-remote/issues"
  },
  "icon": "icon.png",
  "keywords": [
    "cursor",
    "remote",
    "mobile",
    "websocket",
    "cli",
    "agent"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cursorRemote.start",
        "title": "Start Cursor Remote Server"
      },
      {
        "command": "cursorRemote.stop",
        "title": "Stop Cursor Remote Server"
      },
      {
        "command": "cursorRemote.toggle",
        "title": "Toggle Cursor Remote Server"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.74.0",
    "@types/ws": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "ws": "^8.19.0"
  }
}
